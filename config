export SERVICE_NAME=eunomia

export SERVICE_EUNOMIA_IMAGE_REGISTRY=${SERVICE_EUNOMIA_IMAGE_REGISTRY}
export SERVICE_EUNOMIA_IMAGE_ORG=${SERVICE_EUNOMIA_IMAGE_ORG}

export SERVICE_EUNOMIA_IMAGE_FRONTEND_VERSION_DEFAULT=v1.6.1
export SERVICE_EUNOMIA_IMAGE_BACKEND_VERSION_DEFAULT=v1.6.3

export SERVICE_EUNOMIA_IMAGE_FRONTEND_VERSION=${SERVICE_EUNOMIA_IMAGE_FRONTEND_VERSION:-$FRONTEND_VERSION}
export SERVICE_EUNOMIA_IMAGE_BACKEND_VERSION=${SERVICE_EUNOMIA_IMAGE_BACKEND_VERSION:-$BACKEND_VERSION}

if [ -z "$SERVICE_EUNOMIA_IMAGE_FRONTEND_VERSION" ]; then
  export SERVICE_EUNOMIA_IMAGE_FRONTEND_VERSION=$SERVICE_EUNOMIA_IMAGE_FRONTEND_VERSION_DEFAULT
fi

if [ -z "$SERVICE_EUNOMIA_IMAGE_BACKEND_VERSION" ]; then
  export SERVICE_EUNOMIA_IMAGE_BACKEND_VERSION=$SERVICE_EUNOMIA_IMAGE_BACKEND_VERSION_DEFAULT
fi

# export SERVICE_EUNOMIA_HTTP_PORT=${SERVICE_EUNOMIA_HTTP_PORT}
